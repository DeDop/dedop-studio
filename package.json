{
  "name": "dedop-studio",
  "productName": "DeDop",
  "version": "0.0.1",
  "description": "DeDop Studio",
  "private": true,
  "scripts": {
    "start": "electron app",
    "test:e2e": "mocha e2e/**/*-test.js",
    "pretest": "npm run compile",
    "test": "mocha app/main/**/*.spec.js app/renderer/**/*.spec.js",
    "postinstall": "install-app-deps",
    "compile": "tsc",
    "compile:trace": "tsc --traceResolution",
    "dist": "build --x64 --publish never",
    "pack": "build --x64 --publish never --dir",
    "clean": "rimraf app/main app/renderer dist",
    "clean:all": "npm run clean && rimraf node_modules app/node_modules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeDop/dedop-studio"
  },
  "keywords": [
    "dedop",
    "studio",
    "delay",
    "doppler",
    "altimetry"
  ],
  "author": "Brockmann Consult GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DeDop/dedop-studio/issues"
  },
  "homepage": "http://dedop.org/",
  "devDependencies": {
    "@blueprintjs/core": "^1.0.1",
    "@blueprintjs/table": "^1.0.1",
    "@types/chai": "^3.4.34",
    "@types/dom4": "^1.5.20",
    "@types/electron": "^1.4.26",
    "@types/enzyme": "^2.5.38",
    "@types/mocha": "^2.2.32",
    "@types/react": "^0.14.46",
    "@types/react-dom": "^0.14.18",
    "@types/react-redux": "^4.4.32",
    "@types/redux": "^3.6.31",
    "cesium": "^1.27.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "electron-builder": "latest",
    "electron-prebuilt": "1.4.5",
    "enzyme": "^2.6.0",
    "mocha": "^3.1.2",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.5",
    "react-split-pane": "^0.1.52",
    "redux": "^3.6.0",
    "redux-log": "^2.0.0",
    "rimraf": "^2.5.4",
    "spectron": "^3.4.0",
    "ts-node": "^1.7.0",
    "typescript": "^2.0.8"
  },
  "build": {
    "appId": "org.esa.dedop-studio",
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "app/resources/win32/dedop.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerHeaderIcon": "app/resources/win32/dedop.ico"
    },
    "mac": {
      "icon": "app/resources/darwin/dedop.icns"
    },
    "dmg": {
      "icon": "app/resources/darwin/dedop.icns"
    },
    "extraFiles": [
      "app/dedop-core/*/**"
    ]
  }
}
